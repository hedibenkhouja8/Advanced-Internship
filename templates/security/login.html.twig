<div class="card form" style="width: 30rem;">
  <div class="card-body">
  <form class="" action="{{ path('security_login') }}" method="post">
    <h2 class="card-title">Login to get access</h2><br>
    <p class="card-text"><label for="exampleInputEmail1">Email address</label>
    <input type="email" placeholder="xyz@gmail.com" require name="_username" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
</p>
<p class="card-text"> <label for="exampleInputPassword1">Password</label>
    <input type="password" placeholder="Password" require name="_password"  class="form-control" id="exampleInputPassword1">
</p><br>

 <p> <button type="submit" class="btn btn-info">               Login</button>&nbsp;&nbsp;Or&nbsp;&nbsp;
    <a href="/registration" class="card-link btn btn-primary">Register Here</a></p>
  </div>
  </form>
</div>

